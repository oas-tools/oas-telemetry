{
  "presets": [
    [
      "@babel/preset-env",
      { 
        "modules": "commonjs",
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "babel-plugin-transform-import-meta",
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-object-rest-spread",
    ["module-extension", { "js": "cjs"}],
    "add-module-exports"
  ]
}