{
  "name": "oas-telemetry-workspace",
  "description": "Collection packages for OAS-Telemetry",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev -w packages/lib\" \"npm run dev -w packages/ui\"",
    "clean": "rm -rf packages/lib/dist",
    "build": "npm run build -w packages/ui && npm run build -w packages/lib",
    "build:esm": "npm run build:esm -w packages/lib",
    "build:cjs": "npm run build:cjs -w packages/lib",
    "build:ui": "npm run build -w packages/ui",
    "publishToNpm": "npm run build && npm publish"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "apipecker": "^1.5.0",
    "axios": "^1.10.0",
    "concurrently": "^9.1.2"
  }
}